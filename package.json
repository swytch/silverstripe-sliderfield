{
  "name": "silverstripe-sliderfield",
  "version": "2.0.0",
  "description": "Simple slider field for Silverstripe",
  "scripts": {
    "build": "yarn && rm -rf client/dist/* && NODE_ENV=production webpack --mode production --bail --progress",
    "dev": "NODE_ENV=development webpack --progress",
    "watch": "NODE_ENV=development webpack --watch --progress",
    "css": "WEBPACK_CHILD=css npm run build",
    "lint": "eslint client/src; sass-lint -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tractorcow/silverstripe-sliderfield.git"
  },
  "keywords": [
    "silverstripe",
    "slider",
    "jqueryui"
  ],
  "author": "Damian Mooyman",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/tractorcow/silverstripe-sliderfield/issues"
  },
  "homepage": "https://github.com/dnadesign/silverstripe-elemental#readme",
  "engines": {
    "node": "^18.x"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.0",
    "@silverstripe/eslint-config": "^1.0.0-alpha6",
    "@silverstripe/webpack-config": "^2.0.0-alpha9",
    "babel-jest": "^29.2.2",
    "jest-cli": "^29.2.2",
    "jest-environment-jsdom": "^29.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "bootstrap": "^4.6.2",
    "classnames": "^2.2.5",
    "core-js": "^3.26.0",
    "prop-types": "^15.8.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "reactstrap": "^8.9.0",
    "redux": "^4.2.0",
    "redux-form": "^8.3.8",
    "redux-thunk": "^2.4.1",
    "webpack-sources": "^3.2.3"
  }
}
